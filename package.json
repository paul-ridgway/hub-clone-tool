{
  "name": "hub-clone-tool",
  "version": "1.0.2",
  "description": "A little helper to clone github repos",
  "main": "src/index.ts",
  "bin": {
    "hct": "./lib/index.js",
    "hub-clone-tool": "./lib/index.js"
  },
  "scripts": {
    "start": "ts-node-dev --respawn --debug --transpileOnly src/index.ts",
    "build": "tsc -p .",
    "check": "npm run compile && npm run lint:all",
    "compile:watch": "tsc --watch --noEmit --skipLibCheck",
    "compile": "tsc --noEmit --skipLibCheck",
    "lint": "tslint --project .",
    "lint:fix": "tslint --project . --fix",
    "prettify": "prettier --write $(find src -name '*.ts')"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/paul-ridgway/hub-clone-tool.git"
  },
  "keywords": [
    "github",
    "clone"
  ],
  "author": "Paul Ridgway",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paul-ridgway/hub-clone-tool/issues"
  },
  "homepage": "https://github.com/paul-ridgway/hub-clone-tool#readme",
  "dependencies": {
    "@octokit/rest": "^18.0.0",
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/listr": "^0.14.2",
    "@types/parse-git-config": "^3.0.0",
    "@types/zen-observable": "^0.8.0",
    "chalk": "^4.1.0",
    "inquirer": "^7.2.0",
    "install": "^0.13.0",
    "listr": "^0.14.3",
    "npm": "^6.14.5",
    "parse-git-config": "^3.0.0",
    "rxjs": "^6.5.5",
    "simple-git": "^2.11.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.49",
    "tslint": "^6.1.2",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.9.5"
  }
}
